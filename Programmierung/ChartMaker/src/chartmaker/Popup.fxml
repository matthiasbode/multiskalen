<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?> 

<VBox id="popupRoot" onKeyTyped="#handleOKButtonAction"  spacing="5.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="chartmaker.PopupController">
    
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding> 
    <children>
        <Text fx:id="popupText" text="Text not Set"/>
       
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
            <children>
                <Button fx:id="okButton"  mnemonicParsing="false" text="OK" onAction="#handleOKButtonAction"  />
            </children>
        </HBox>
    </children>
    
    <stylesheets>
        <URL value="@ChartMakerMain.css"/>
    </stylesheets>
</VBox>
